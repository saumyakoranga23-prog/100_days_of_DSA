#include <stdio.h>
#include <stdlib.h>

struct Node {
    int data;
    struct Node* next;
};

struct Node* insert(struct Node* head, int x) {
    struct Node* n = malloc(sizeof(struct Node));
    n->data = x;
    n->next = NULL;

    if (head == NULL) return n;

    struct Node* t = head;
    while (t->next != NULL)
        t = t->next;
    t->next = n;
    return head;
}

struct Node* merge(struct Node* a, struct Node* b) {
    struct Node dummy;
    struct Node* tail = &dummy;
    dummy.next = NULL;

    while (a && b) {
        if (a->data <= b->data) {
            tail->next = a;
            a = a->next;
        } else {
            tail->next = b;
            b = b->next;
        }
        tail = tail->next;
    }

    if (a) tail->next = a;
    if (b) tail->next = b;

    return dummy.next;
}

int main() {
    int n, m, x;
    struct Node *h1 = NULL, *h2 = NULL, *res;

    scanf("%d", &n);
    for (int i = 0; i < n; i++) {
        scanf("%d", &x);
        h1 = insert(h1, x);
    }

    scanf("%d", &m);
    for (int i = 0; i < m; i++) {
        scanf("%d", &x);
        h2 = insert(h2, x);
    }

    res = merge(h1, h2);

    while (res) {
        printf("%d ", res->data);
        res = res->next;
    }

    return 0;
}
